{
    "bpbf": "",
    "zoom" : 5,
    "style":
    "
  #main {
    marker-width: 6;
    marker-line-width: 1;

    [zoom >= 5] {
      [count = 1] {
        marker-width: 6;
      }
      [count > 1] {
        marker-width: 8;
      }
      [count > 3] {
        marker-width: 10;
      }
      [count > 5] {
        marker-width: 12;
      }
    }

    [zoom < 5] {
      [count = 1] {
        marker-width: 12;
      }
      [count > 1] {
        marker-width: 16;
      }
      [count > 5] {
        marker-width: 20;
      }
      [count > 10] {
        marker-width: 24;
      }

      marker-line-width: 3;
    }

    marker-fill: #FFF;
    marker-line-color: #FFF;
    marker-line-opacity: 1;

    [primary_type = 'THEFT'] {
      marker-line-color: #000;
    }

    [primary_type = 'ASSAULT'] {
      marker-line-color: #F00;
    }

    [primary_type = 'NARCOTICS'] {
      marker-line-color: #0F0;
    }

    [primary_type = 'DECEPTIVE PRACTICE'] {
      marker-line-color: #00F;
    }

    [arrest = 'true'] {
      marker-fill: #33F;
    }
  }
    "
}
